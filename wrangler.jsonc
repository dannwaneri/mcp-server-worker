{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "mcp-server-worker",
	"main": "src/index.ts",
	"compatibility_date": "2025-12-02",
	"compatibility_flags": ["nodejs_compat"],
	"observability": {
		"enabled": true
	},
	"ai": {
		"binding": "AI"
	},
	"vectorize": [
		{
			"binding": "VECTORIZE",
			"index_name": "mcp-knowledge-base"
		}
	]
}